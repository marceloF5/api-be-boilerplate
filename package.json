{
  "name": "api-boilerplate",
  "version": "1.0.0",
  "description": "API Boilerplate",
  "main": "./server/server.ts",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "test": "NODE_ENV=test node ./dist/server/server.js",
    "dist": "NODE_ENV=prodction node ./dist/server/server.js",
    "unit-test": "NODE_ENV=test mocha --opts dist/test/unit/config/mocha.opts ./dist/tests/unit/unit.test.js",
    "integration-test": "NODE_ENV=test mocha --opts dist/test/integration/config/mocha.opts ./dist/tests/integration/integration.test.js",
    "integration-coverage": "nyc --reporter=html npm run integration-test",
    "unit-coverage": "nyc --reporter=html npm run unit-test"
  },
  "keywords": [
    "API",
    "Typecript",
    "Node",
    "ES6"
  ],
  "author": "Marcelo Fortunato",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "http": "0.0.0",
    "http-status": "^1.0.1",
    "jwt-simple": "^0.5.1",
    "morgan": "^1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "pg": "^7.4.1",
    "sequelize": "^4.33.4",
    "sequelize-cli": "^3.2.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/body-parser": "^1.16.8",
    "@types/chai": "^4.1.2",
    "@types/express": "^4.11.1",
    "@types/http-status": "^0.2.30",
    "@types/jwt-simple": "^0.5.33",
    "@types/mocha": "^2.2.48",
    "@types/morgan": "^1.7.35",
    "@types/passport": "^0.4.3",
    "@types/passport-jwt": "^3.0.1",
    "@types/sequelize": "^4.27.5",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.4.0",
    "gulp-typescript": "^4.0.1",
    "istanbul": "^0.4.5",
    "lodash": "^4.17.5",
    "mocha": "^5.0.0",
    "nyc": "^11.6.0",
    "supertest": "^3.0.0",
    "testdouble": "^3.5.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
  }
}
